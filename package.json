    "moment": "^2.29.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
    "npm-run-series": "^1.0.1",
    "pg": "^7.14.0",
    "xls-to-json-lc": "^0.3.4",
    "xlsx-to-json-lc": "^0.5.0"
  },
